{
  "name": "schematization",
  "version": "1.0.0",
  "description": "Implementation of the area-preserving simplification and schematization of polygonal subdivisions, proposed by Buchin et al. (2016)",
  "repository": "https://github.com/jakoblistabarth/area-preserving-polygon-schematization",
  "main": "index.js",
  "scripts": {
    "test": "jasmine",
    "build": "esbuild --bundle assets/main.js --sourcemap --outfile=dist/main.bundle.js",
    "dev": "nodemon",
    "start": "browser-sync --config config/bs-config.js"
  },
  "nodemonConfig": {
    "ignore": [
      "spec/*",
      "dist/*"
    ],
    "exec": "npm run build"
  },
  "pre-commit": [
    "test"
  ],
  "author": "Jakob Listabarth",
  "license": "ISC",
  "dependencies": {
    "@mapbox/geojsonhint": "^3.0.1",
    "browser-sync": "^2.26.14",
    "esbuild": "^0.11.9",
    "graham_scan": "^1.0.4",
    "jasmine": "^3.7.0",
    "leaflet": "^1.7.1",
    "nodemon": "^2.0.7",
    "shpjs": "^3.6.3",
    "turf": "^3.0.14",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2"
  }
}
